{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "type": "module",
    "service_worker": "dist/sw.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "dist/popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.nytimes.com/games/wordle/*"],
      "js": ["dist/main.js"]
    }
  ]
}
